  "tokenColors": [
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ff5370"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a9c0d660"
      }
    },
    {
      "name": "Number, Constant",
      "scope": ["constant.numeric", "constant.character", "constant.escape"],
      "settings": {
        "foreground": "#EF8195"
      }
    },
    {
      "name": "String, Symbols",
      "scope": ["string"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#0A92AE"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage", "constant.language", "support.type.builtin"],
      "settings": {
        "foreground": "#0DB5D7"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#EA5D76"
      }
    },

    {
      "name": "Keyword Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#EA5D76DD"
      }
    },
    {
      "name": "Decorator",
      "scope": ["meta.decorator"],
      "settings": {
        "foreground": "#EA5D76"
      }
    },
    {
      "name": "Decorator",
      "scope": ["punctuation.decorator"],
      "settings": {
        "foreground": "#F193A4"
      }
    },
    {
      "name": "Object",
      "scope": ["meta.object"],
      "settings": {
        "foreground": "#a9c0d6"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other.enummember"],
      "settings": {
        "foreground": "#a9c0d6"
      }
    },
    {
      "name": "Variables Other",
      "scope": [
        "variable.other",
        "meta.parameters variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#dac6a2"
      }
    },
    {
      "name": "Method",
      "scope": [
        "meta.method",
        "meta.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#dac6a2"
      }
    },
    {
      "name": "Class, Entity, Alias, Namespace",
      "scope": [
        "meta.class",
        "variable.other.object",
        "variable.other.readwrite.alias",
        "meta.attribute entity.name",
        "entity.name.package",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Type, Enum, Interface",
      "scope": [
        "meta.type",
        "meta.interface",
        "meta.enum",
        "support.type",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Tag Name",
      "scope": ["meta.tag entity.name.tag"],
      "settings": {
        "foreground": "#3ED6F4"
      }
    },
    {
      "name": "Tag Markup",
      "scope": ["meta.tag punctuation.definition.tag"],
      "settings": {
        "foreground": "#0A92AE"
      }
    },
    {
      "name": "Tag attributes",
      "scope": ["meta.tag.attributes", "meta.attribute"],
      "settings": {
        "foreground": "#0DB5D7"
      }
    },
    {
      "name": "Tag expression",
      "scope": ["meta.tag.attributes meta.embedded.expression"],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "YML",
      "scope": ["entity.name.tag.yaml"],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "YML unquoted",
      "scope": ["string.unquoted.plain.out.yaml", "source.yaml punctuation"],
      "settings": {
        "foreground": "#a9c0d6"
      }
    },
    {
      "name": "JSON property",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "source.css support.type.property-name",
