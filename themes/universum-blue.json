  "tokenColors": [
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#EA5D76"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#A68FD8"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#A68FD8"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#0A92AE"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "unused.comment",
        "wildcard.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b7bfd360"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#929dbb"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#82c379"
      }
    },
    {
      "name": "String - Punctuation",
      "scope": ["string punctuation"],
      "settings": {
        "foreground": "#aec4aa"
      }
    },
    {
      "name": "String - Template expression",
      "scope": ["punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#0DB5D7"
      }
    },
    {
      "name": "Number, Constant",
      "scope": ["constant.numeric", "constant.character", "constant.escape"],
      "settings": {
        "foreground": "#0DB5D7"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#0DB5D7"
      }
    },
    {
      "name": "Language",
      "scope": ["constant.language", "support.type.builtin"],
      "settings": {
        "foreground": "#0DB5D7"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator",
        "meta.decorator",
        "punctuation.decorator",
        "punctuation.definition.keyword",
        "support.type.object.module",
        "variable.language"
      ],
      "settings": {
        "foreground": "#EA5D76"
      }
    },
    {
      "name": "Variables, Objects",
      "scope": [
        "variable",
        "meta.object",
        "variable.other.property",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#b7bfd3"
      }
    },
    {
      "name": "Variables - Others",
      "scope": ["variable.other"],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Variables - Arguments",
      "scope": ["meta.parameters variable", "variable.parameter"],
      "settings": {
        "foreground": "#cfc2abff"
      }
    },
    {
      "name": "Method",
      "scope": ["meta.method", "meta.function", "support.function"],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Method - Builtin",
      "scope": ["support.function.builtin"],
      "settings": {
        "foreground": "#0DB5D7"
      }
    },
    {
      "name": "Class, Entity, Alias, Namespace",
      "scope": [
        "meta.class",
        "variable.other.object",
        "variable.other.readwrite.alias",
        "meta.attribute entity.name",
        "entity.name.package",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#E5C07B"
      }
