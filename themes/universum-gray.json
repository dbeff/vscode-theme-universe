  "tokenColors": [
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#E65C75"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#0EC2E6"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#6D97B0"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "unused.comment",
        "wildcard.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b9bec960"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#b9bec9"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#88CC7E"
      }
    },
    {
      "name": "String - Punctuation",
      "scope": ["string punctuation"],
      "settings": {
        "foreground": "#b3ceaf"
      }
    },
    {
      "name": "String - Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#BF98E6"
      }
    },
    {
      "name": "String - Template expression",
      "scope": ["punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#0EC2E6"
      }
    },
    {
      "name": "Number, Constant",
      "scope": ["constant.numeric", "constant.character", "constant.escape"],
      "settings": {
        "foreground": "#0EC2E6"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "constant.language",
        "support.type.builtin",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#0EC2E6"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator",
        "meta.decorator",
        "punctuation.decorator",
        "punctuation.definition.keyword",
        "support.type.object.module",
        "variable.language"
      ],
      "settings": {
        "foreground": "#E65C75"
      }
    },
    {
      "name": "Variables - Class, Entity, Alias, Namespace",
      "scope": [
        "variable",
        "variable.other.readwrite.alias",
        "meta.class",
        "meta.attribute entity.name",
        "meta.definition.property",
        "entity.name.package",
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Variable - Objects, Properties",
      "scope": [
        "meta.object",
        "variable.object",
        "variable.other.property",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#b9bec9"
      }
    },
    {
      "name": "Variables - Arguments",
      "scope": ["meta.parameters variable", "variable.parameter"],
      "settings": {
        "foreground": "#BF98E6"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#E5C07B"
